<h1>Edit "<%= @supplier.name %>"</h1>
<form method="post" action="/suppliers/<%= @supplier.id %>">
    <label for="name">Name:</label>
    <input id="name" type="text" name="name" value="<%= @supplier.name %>">

    <label for "picture"> Link to map:</label>
    <input id="picture" type="text" name="picture">

  <input type="submit" value="Update Supplier">
</form>